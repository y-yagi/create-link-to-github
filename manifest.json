{
  "name": "Create link to GitHub",
  "version": "0.0.5",
  "manifest_version": 3,
  "description": "Create a link to GitHub with markdown",
  "permissions": [
    "tabs",
    "contextMenus"
  ],
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["script.js"]
  }],
  "background": {
    "service_worker": "background.js"
  }
}
